  "version": "0.1.0",
  "type": "module",
  "author": "Charley Reeson (https://chazza.me)",
  "repository": {
    "url": "https://github.com/edenchazard/vue-angry-select"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "run-p ts:check \"build-only {@}\" --",
